<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<profiles>
		<profile>
			<id>github</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<altDeploymentRepository>github::default::https://maven.pkg.github.com/enterpriseDomain/ClassMaker/</altDeploymentRepository>
				<altReleaseDeploymentRepository>github-release::default::https://maven.pkg.github.com/enterpriseDomain/ClassMaker/</altReleaseDeploymentRepository>
				<altSnapshotDeploymentRepository>github-snapshot::default::https://maven.pkg.github.com/enterpriseDomain/ClassMaker/</altSnapshotDeploymentRepository>
			</properties>
			<repositories>
				<repository>
					<id>github-snapshot</id>
					<name>GitHub snapshot</name>
					<releases>
						<enabled>false</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>
					<url>https://maven.pkg.github.com/enterpriseDomain/ClassMaker</url>
					<layout>default</layout>
				</repository>
				<repository>
					<id>artifact-repository</id>
					<!-- <id>github-release</id> -->
					<name>GitHub release</name>
					<releases>
						<enabled>true</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
					<snapshots>
						<enabled>false</enabled>
						<updatePolicy>always</updatePolicy>
						<checksumPolicy>fail</checksumPolicy>
					</snapshots>
					<url>https://maven.pkg.github.com/enterpriseDomain/ClassMaker</url>
					<layout>default</layout>
				</repository>
				<repository>
					<id>central</id>
					<url>https://repo1.maven.org/maven2</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>
		</profile>
	</profiles>
	<servers>
		<server>
			<id>github</id>
			<username>${github.user}</username>
			<password>${github.password}</password>
		</server>
		<server>
			<id>github-snapshot</id>
			<username>${github.user}</username>
			<password>${github.password}</password>
		</server>
		<server>
			<id>artifact-repository</id>
			<!-- <id>github-release</id> -->
			<username>${github.user}</username>
			<password>${github.password}</password>
		</server>
	</servers>
</settings>
